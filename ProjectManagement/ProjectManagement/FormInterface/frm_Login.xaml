<Window x:Class="ProjectManagement.FormInterface.frm_Login"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectManagement.FormInterface"
             mc:Ignorable="d" 
             Height="300" Width="300" Title="Login">
     
        <StackPanel>
            <TextBlock Margin="3">Email</TextBlock>
            <TextBox Name="email" Margin="3" Text="123456@gmail.com" ></TextBox>
            <TextBlock  Margin="3">Password</TextBlock>
        <PasswordBox Name="password"   Margin="3" PasswordChar="*" Password="123456"></PasswordBox>
           
        <Button Name="btnLogin" Margin="3" Click="Button_Click"  >Login</Button>
        </StackPanel>
     
</Window>
