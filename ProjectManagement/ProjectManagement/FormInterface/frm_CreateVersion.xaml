<Window x:Class="ProjectManagement.FormInterface.frm_CreateVersion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectManagement.FormInterface"
             mc:Ignorable="d" 
              Height="150"  Width="400">
    <Grid>
        <StackPanel >
            <TextBlock Margin="3">Auteur</TextBlock>
            <TextBox Name="txbAuteur" Margin="3"></TextBox>
            <TextBlock Margin="3">Commentaires</TextBlock>
            <TextBox Name="txbComment" Margin="3"></TextBox>
            <Button Click="Button_Click">Create Version</Button>

        </StackPanel>  
    </Grid>
</Window>
