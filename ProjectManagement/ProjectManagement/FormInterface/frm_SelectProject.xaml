<Window x:Class="ProjectManagement.FormInterface.frm_SelectProject"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectManagement.FormInterface"
             mc:Ignorable="d" 
             Height="200" Width="350">
    <Grid>
    <StackPanel>
        <TextBlock Margin="3">Select project Database</TextBlock>
        <ComboBox Name="cbSelectProject"  Margin="3" SelectionChanged="CbSelectProject_SelectionChanged"></ComboBox>
        <TextBlock Margin="3">Select project Revit</TextBlock>
           <ComboBox Margin="3" Name="cbSelectProjectRevit" SelectionChanged="CbSelectProjectRevit_SelectionChanged"></ComboBox>
        <Button Name="btnOK" Margin="3" Click="Button_Click" IsEnabled="False">OK</Button>
    </StackPanel>
    </Grid>
</Window>
